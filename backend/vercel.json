{
    "version": 2,
    "functions": {
        "src/main.ts": {
            "runtime": "@vercel/node@2.0.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
